{
	"compilerOptions": {
		"module": "esnext",
		"target": "es5",
		"noImplicitAny": true,
		"moduleResolution": "node",
		"strict": true,
		"noLib": true,
		"typeRoots": [],
	},
	"exclude": [
		"node_modules",
	],
	"baseUrl": "./",
	"paths": {
		"@src": [
			"./src/*"
		],
		"@custom_modules": [
			"./custom_modules/*"
		],
		"@types": [
			"./types/*"
		],
	}
}